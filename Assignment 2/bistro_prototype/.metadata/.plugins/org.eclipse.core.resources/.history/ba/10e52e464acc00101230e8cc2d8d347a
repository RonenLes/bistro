package common;

import entities.Reservation;

public class ReservationResponse {

	private boolean isReservationSuccess;
	private String msg;
	private Reservation reservation;
	
	
	public ReservationResponse(boolean isReservationSuccess, String msg, int confirmationCode) {
		this.isReservationSuccess= isReservationSuccess;
		this.msg= msg;
		this.confirmationCode=confirmationCode;
	}
	
	public boolean getIsReservationSuccess() {
		return this.isReservationSuccess;
	}
	
	public String msg() {
		return this.msg;
	}
	
	public int getConfirmationCode() {
		return this.confirmationCode;
	}
}
